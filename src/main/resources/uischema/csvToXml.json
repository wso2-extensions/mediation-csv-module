{
  "connectorName": "CSV",
  "operationName": "csvToXml",
  "title": "CSV to XML",
  "help": "<h1>CSV to XML</h1> <b>Convert a CSV payload to XML</b>",
  "elements": [
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "General",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "description",
              "displayName": "Description",
              "inputType": "string",
              "defaultValue": "",
              "required": "false",
              "helpTip": "Component description"
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "CSV Input",
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "headerPresent",
                    "displayName": "Header",
                    "inputType": "comboOrExpression",
                    "defaultValue": "Absent",
                    "required": "false",
                    "comboValues": [
                      "Present",
                      "Absent"
                    ],
                    "helpTip": "Is there a header in the CSV payload ? Default is Absent"
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "valueSeparator",
                    "displayName": "Separator",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "Specify a value separator for CSV. Default is comma ( , )"
                  }
                }
              ]
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "CSV Transformation",
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "skipHeader",
                    "displayName": "Skip Headers",
                    "inputType": "booleanOrExpression",
                    "defaultValue": false,
                    "required": "false",
                    "helpTip": "Skip header from the content. Default is False",
                    "enableCondition": [
                      {
                        "headerPresent": "Present"
                      }
                    ]
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "columnsToSkip",
                    "displayName": "Skip Columns",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "Comma separated string of column indexes to skip. Leave blank to skip no columns"
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "dataRowsToSkip",
                    "displayName": "Skip Data Rows",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "Number of data rows to skip. Default is 0"
                  }
                }
              ]
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "XML Output",
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "tagNames",
                    "displayName": "Tag Names",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "Comma separated string of tag names for result XML"
                  }
                },
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "Root Element",
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "rootElementTag",
                          "displayName": "Tag",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "Tag name of the root XML element"
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "rootElementNamespace",
                          "displayName": "Namespace",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "Namespace URI for root XML element"
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "rootElementNamespaceURI",
                          "displayName": "Namespace URI",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "Namespace prefix for root XML element"
                        }
                      }
                    ]
                  }
                },
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "Group Element",
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "groupElementTag",
                          "displayName": "Tag",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "Name of the group xml elements"
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "groupElementNamespace",
                          "displayName": "Namespace",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "Namespace URI for group XML element"
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "groupElementNamespaceURI",
                          "displayName": "Namespace URI",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "Namespace prefix group XML element"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
